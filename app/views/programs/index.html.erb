<table>
  <thead>
    <tr>
      <th class="header" colspan="2">
        <h1 class="headerText">Programs</h1>
      </th>
    </tr>
  </thead>  

  <tbody class="bodyRow">
    <% @programs.each_with_index do |program, index| %>
      <tr>
        <td id='<%="program-#{index}"%>'></td>
        <td><%= link_to program.name, program %></td>
      </tr>
    <% end %>
  </tbody>
</table>

