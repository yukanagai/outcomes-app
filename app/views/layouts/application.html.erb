<!DOCTYPE html>
<html>
<head>
  <title>OutcomesApp :: <%= controller_name %></title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,700,300,300italic' rel='stylesheet' type='text/css'>
	<link href='https://fonts.googleapis.com/css?family=Pacifico' rel='stylesheet' type='text/css'>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/1.0.2/Chart.js"></script>
    <link rel="stylesheet" href="animate.min.css">
</head>

<body id="<%= controller_name %>_<%= action_name %>" class="<%= controller_name %>">

<div class="container">
	<div id="sidebar">
		<div id="catlogo"></div>
  	<ul>
      <li><a href="/contacts">Contacts</a></li>
      <li><a href="/programs">Programs</a></li>
      <li><a href="/cohorts">Cohorts
      <li><a href="/logout">Logout</a></li>
   	 </ul>
    </div>
	<div class="navbar">
		<div class="main-content">
	     <a href="#" data-toggle=".container" id="sidebar-toggle">
        <span class="bar"></span>
        <span class="bar"></span>
        <span class="bar"></span>
	     </a>
       <div id="logobluesmall">Catcademy</div>
	   </div>
  </div>

  <div class="content">
    <p id="notice"><%= notice %></p>
  	<%= yield %>  
  </div>

</div>

</body>
</html>
