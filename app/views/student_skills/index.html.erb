<p id="notice"><%= notice %></p>

<h1>Listing Student Skills</h1>

<table>
  <thead>
    <tr class="headerRow">
      <th>Student</th>
      <th>Skill</th>
      <th>Experience</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="bodyRow">
    <% @student_skills.each do |student_skill| %>
      <tr>
        <td><%= student_skill.student %></td>
        <td><%= student_skill.skill %></td>
        <td><%= student_skill.experience %></td>
        <td><%= link_to 'Show', student_skill %></td>
        <td><%= link_to 'Edit', edit_student_skill_path(student_skill) %></td>
        <td><%= link_to 'Destroy', student_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student skill', new_student_skill_path %>

<div id="catlogo"></div>
